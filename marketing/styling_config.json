  "statistics_box": {
    "background_color": "chart_background",
    "border_color": "chart_border",
    "border_radius": 0.15,
    "padding": 0.2,
    "shadow_enabled": true
  },
  "header": {
    "background_gradient": true,
    "gradient_start": "background_primary",
    "gradient_end": "background_secondary",
    "text_color": "text_primary",
    "alignment": "center"
  },
  "footer": {
    "background_gradient": true,
    "gradient_start": "background_primary",
    "gradient_end": "background_secondary",
    "text_color": "text_secondary",
    "alignment": "center",
    "text": "Acadexis Salary Benchmarking Report | Confidential"
  },
  "branding": {
    "logo_enabled": false,
    "logo_path": null,
    "logo_position": "top_right",
    "logo_width": 1.0
  }
