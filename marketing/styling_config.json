    "header_height_ratio": 0.12,
    "footer_height_ratio": 0.06,
    "content_height_ratio": 0.82,
    "chart_width_ratio": 0.65,
    "stats_width_ratio": 0.35,
    "spacing_between_elements": 0.2
  },
  "chart": {
    "background_color": "chart_background",
    "border_color": "chart_border",
    "border_radius": 0.15,
    "border_width": 1,
    "shadow_enabled": true,
    "shadow_blur": 12,
    "shadow_offset": 4,
    "padding": 0.3,
    "grid_enabled": true,
    "grid_style": "--",
    "grid_color": "#cccccc",
    "grid_alpha": 0.3
