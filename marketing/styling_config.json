{
  "colors": {
    "background_primary": "#1a3a5c",
    "background_secondary": "#2d5a7b",
    "enrollment_small": "#7eb3d4",
    "enrollment_large": "#4a1a8c",
    "enrollment_medium": "#2563a8",
    "text_primary": "#ffffff",
    "text_secondary": "#e8f4f8",
    "chart_background": "#f5f5f5",
    "chart_border": "#cccccc",
    "accent_line": "#5eb3d4",
    "positive": "#2ecc71",
    "negative": "#e74c3c"
  },
  "typography": {
    "main_title": {
      "size": 28,
      "weight": "bold",
      "family": "sans-serif",
      "color": "text_primary"
    },
    "subtitle": {
      "size": 14,
      "weight": "normal",
      "family": "sans-serif",
      "color": "text_secondary"
    },
    "chart_title": {
      "size": 16,
      "weight": "bold",
      "family": "sans-serif",
      "color": "#333333"
    },
    "callout": {
      "size": 12,
      "weight": "normal",
      "family": "sans-serif",
      "color": "text_primary"
    },
    "statistics_label": {
      "size": 10,
      "weight": "bold",
      "family": "sans-serif",
      "color": "#333333"
    },
    "statistics_value": {
      "size": 11,
      "weight": "normal",
      "family": "sans-serif",
      "color": "#333333"
    }
  },
  "layout": {
    "page_width": 11,
    "page_height": 8.5,
    "margins": {
      "top": 0.5,
      "bottom": 0.3,
      "left": 0.5,
      "right": 0.5
