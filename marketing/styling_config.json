  "dumbbell_chart": {
    "line_color_positive": "positive",
    "line_color_negative": "negative",
    "state_dot_color": "#95a5a6",
    "school_dot_color": "#3498db",
    "dot_size": 400,
    "line_width": 3,
    "line_alpha": 0.6
